<h1>Ships</h1>

<table>
  <thead>
    <tr>
      <th> IMO  </th>
      <th> Name </th>
      <th> Status </th>
      <th>Tipo de Ship</th>
      <th>Company_id</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @ships.each do |ship| %>
      <tr>
        <td><%= ship.imo %></td>
        <td><%= ship.name %></td>
        <td><%= ship.status %></td>
        <td><%= ship.type_ship %></td>
         <td><%= ship.company_id %></td>       
      </tr>
    <% end %>
  </tbody>
</table>
